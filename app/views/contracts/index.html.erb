<br />
<table border = 1>
  <tr>
    <th>Фамилия</th>
    <th>Имя</th>
    <th>Отчество</th>
    <th>Должность</th>
    <th>Действия</th>
  </tr>
<% for contract in @contracts %>
  <tr>
    
    <td><%=h contract.surname%></td>
    <td><%=h contract.name%></td>
    <td><%=h contract.lastname%></td>
    <% k='преподаватель' %>
    <td> <%=h k %>  </td>

    <td><%= link_to 'Подробнее', contract_path (contract) %> |
    <%= link_to 'Удалить', contract_path ( contract), :confirm => 'Вы уверены?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<a href="javascript:history.back();">Назад</a>

