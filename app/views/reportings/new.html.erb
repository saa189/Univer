<h1>Новая отчетность</h1>

<% form_for([@discipline, @reporting]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :Вид %><br />
    <%= f.text_field :rtype %>
  </p>
  <p>
<% @reporting.discipline_id == @discipline.id %>
  </p>
  <p>
    <%= f.submit 'Создать' %>
  </p>
<% end %>

<%= link_to 'Назад', discipline_reportings_path (@discipline) %>
